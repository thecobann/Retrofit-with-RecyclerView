<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/card_view"
    android:layout_margin="5dp"
    app:cardBackgroundColor="#FBF8F8"
    app:cardCornerRadius="12dp"
    app:cardElevation="3dp"
    app:contentPadding="4dp"
    >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:contentPadding="16dp">

        <TextView
            app:layout_constraintBottom_toTopOf="@+id/userId"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/headline_1"
            android:textColor="#CF0101"
            android:layout_width="354dp"
            android:layout_height="20dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="33dp"
            android:layout_marginBottom="10dp"
            android:ems="10"
            android:text="UserId:"
            android:textSize="16sp"/>

        <TextView
            app:layout_constraintBottom_toTopOf="@id/headline_2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/userId"
            android:layout_width="354dp"
            android:layout_height="20dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="33dp"
            android:layout_marginBottom="10dp"
            android:textColor="#000000"
            android:textSize="10sp"
              />
        <TextView
            app:layout_constraintBottom_toTopOf="@+id/id"
            android:id="@+id/headline_2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="#CF0101"
            android:layout_width="354dp"
            android:layout_height="20dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="33dp"
            android:layout_marginBottom="10dp"
            android:ems="10"
            android:text="Id:"
            android:textSize="16sp"/>

        <TextView
            app:layout_constraintBottom_toTopOf="@id/headline_3"
            android:id="@+id/id"
            android:layout_width="354dp"
            android:layout_height="20dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="34dp"
            android:layout_marginBottom="10dp"
            android:textColor="#000000"
            android:textSize="10sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />


        <TextView
            app:layout_constraintBottom_toTopOf="@+id/title"
            android:id="@+id/headline_3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="#CF0101"
            android:layout_width="354dp"
            android:layout_height="20dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="33dp"
            android:layout_marginBottom="10dp"
            android:ems="10"
            android:text="Title:"
            android:textSize="16sp"/>

        <TextView
            app:layout_constraintBottom_toTopOf="@id/headline_4"
            android:id="@+id/title"
            android:layout_width="354dp"
            android:layout_height="50dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="33dp"
            android:layout_marginBottom="10dp"
            android:textColor="#000000"
            android:textSize="10sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            app:layout_constraintBottom_toTopOf="@+id/body"
            android:id="@+id/headline_4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="#CF0101"
            android:layout_width="354dp"
            android:layout_height="20dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="33dp"
            android:layout_marginBottom="10dp"
            android:ems="10"
            android:text="Body:"
            android:textSize="16sp"/>

        <TextView
            app:layout_constraintBottom_toBottomOf="parent"
            android:id="@+id/body"
            android:layout_width="354dp"
            android:layout_height="80dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="33dp"
            android:textColor="#000000"
            android:textSize="10sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />





    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.cardview.widget.CardView>